{
  "name": "web",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:full": "concurrently \"pnpm dev\" \"pnpm payload:watch\"",
    "build": "payload generate:types --silent && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "payload": "payload",
    "payload:generate": "payload generate:types",
    "payload:migrate": "payload migrate",
    "payload:migrate:create": "payload migrate:create",
    "payload:watch": "nodemon --watch 'lib/collections/**/*.ts' --exec 'payload generate:types'",
    "payload:validate": "tsx scripts/validate-types.ts",
    "seed": "tsx scripts/seed.ts development",
    "test": "vitest run --reporter=basic --silent",
    "test:verbose": "vitest run --reporter=basic",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:parallel": "vitest run --reporter=verbose --pool=forks --poolOptions.forks.singleFork=false",
    "test:parallel:watch": "vitest --reporter=verbose --pool=forks --poolOptions.forks.singleFork=false",
    "test:isolated": "vitest run __tests__/seed-isolated.test.ts --reporter=verbose",
    "test:sequential": "vitest run --no-file-parallelism --pool=forks --poolOptions.forks.singleFork=true",
    "prepare": "husky"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.46.0",
    "@payloadcms/next": "^3.46.0",
    "@payloadcms/richtext-lexical": "^3.46.0",
    "@types/node-geocoder": "^4.2.6",
    "@types/papaparse": "^5.3.16",
    "@types/uuid": "^10.0.0",
    "@workspace/ui": "workspace:*",
    "graphql": "^16.11.0",
    "lucide-react": "^0.475.0",
    "next": "^15.2.3",
    "next-themes": "^0.4.4",
    "node-geocoder": "^4.4.1",
    "papaparse": "^5.5.3",
    "payload": "^3.46.0",
    "pg": "^8.16.3",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.2",
    "tsx": "^4.17.0",
    "uuid": "^11.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@workspace/eslint-config": "workspace:^",
    "@workspace/typescript-config": "workspace:*",
    "husky": "^9.1.7",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
