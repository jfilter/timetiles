{
  "name": "web",
  "description": "The web application for TimeTiles, providing the main user interface and data exploration features.",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:debug": "LOG_LEVEL=debug next dev --turbopack",
    "build": "payload generate:types --silent && next build",
    "build:compile": "payload generate:types --silent && next build --experimental-build-mode compile",
    "start": "next start",
    "lint": "eslint app lib components tests scripts . --ext .ts,.tsx,.js,.jsx --cache",
    "format": "eslint app lib components tests scripts . --ext .ts,.tsx,.js,.jsx --cache --fix --quiet && prettier --write . --log-level error",
    "typecheck": "tsc --noEmit",
    "check:ai": "tsx scripts/check-summary.ts",
    "payload": "payload",
    "payload:generate": "tsx scripts/generate-payload.ts",
    "payload:migrate": "payload migrate",
    "payload:migrate:create": "payload migrate:create",
    "payload:validate": "tsx scripts/validate-types.ts",
    "seed": "tsx scripts/seed.ts",
    "test": "vitest run",
    "test:ai": "tsx scripts/test-ai.ts",
    "test:silent": "vitest run --reporter=basic --silent",
    "test:debug": "LOG_LEVEL=debug vitest run --reporter=verbose",
    "test:coverage": "vitest run --coverage --coverage.reportOnFailure=true",
    "test:coverage:summary": "tsx scripts/coverage-summary.ts",
    "test:coverage:check": "tsx scripts/coverage-summary.ts --threshold 80",
    "setup:e2e-db": "tsx scripts/e2e-setup-database.ts",
    "test:e2e": "playwright test",
    "test:e2e:debug": "playwright test --debug --headed",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration"
  },
  "dependencies": {
    "@cfworker/json-schema": "^4.1.1",
    "@payloadcms/db-postgres": "^3.64.0",
    "@payloadcms/next": "^3.64.0",
    "@payloadcms/richtext-lexical": "^3.64.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/maplibre-gl": "^1.14.0",
    "@types/node-geocoder": "^4.2.6",
    "@types/papaparse": "^5.5.0",
    "@types/uuid": "^10.0.0",
    "@workspace/ui": "workspace:*",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "graphql": "^16.12.0",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.554.0",
    "maplibre-gl": "^5.13.0",
    "next": "^15.5.6",
    "next-themes": "^0.4.6",
    "node-geocoder": "^4.4.1",
    "nuqs": "^2.8.1",
    "papaparse": "^5.5.3",
    "payload": "^3.64.0",
    "pg": "^8.16.3",
    "pino": "^9.14.0",
    "pino-pretty": "^13.1.2",
    "quicktype-core": "^23.2.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-map-gl": "^8.1.0",
    "sharp": "^0.34.5",
    "tsx": "^4.20.6",
    "uuid": "^11.1.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^10.0.0",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.5",
    "eslint": "^9.39.1",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "msw": "^2.12.2",
    "playwright": "^1.56.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
